/*
 * Settings and profiles
 */

// Apptainer settings
params.apptainer     = true
apptainer.enabled    = true
apptainer.autoMounts = true

// Singularity settings
//params.apptainer       = false
//singularity.enabled    = true
//singularity.autoMounts = true

// Load default config options
includeConfig 'conf/base.config'

// Load config options for modules
includeConfig 'conf/modules.config'

// Profiles
profiles {
	
	// Standard config options (when no profile is specified)
	standard { process.executor = 'local' }

	// SGE config options
	sge { includeConfig 'conf/sge.config' }

	// Slurm config options
	slurm { includeConfig 'conf/slurm.config' }

	// TSO500 config options
	tso500 { includeConfig 'conf/tso500.config' }

	// OPA config options
	opa { includeConfig 'conf/opa.config' }

	// OCA config options
	oca { includeConfig 'conf/oca.config' }

	// AGL config options
	agl { includeConfig 'conf/agl.config' }

	// BRP config options
	brp { includeConfig 'conf/brp.config' }

	// IDT config options
	idt { includeConfig 'conf/idt.config' }

	// IGT config options
	igt { includeConfig 'conf/igt.config' }

	// ILM config options
	ilm { includeConfig 'conf/ilm.config' }

	// TFS config options
	tfs { includeConfig 'conf/tfs.config' }

	// Custom panel config options
	custom { includeConfig 'conf/custom.config' }

}

// Report and trace (overwrite by default)
report {
    overwrite = true
}
trace {
    overwrite = true
}
