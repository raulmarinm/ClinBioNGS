	cnvkitImg          = "${singularityDir}/cnvkit_${cnvkitVersion}.sif"
	ctatSplicingImg    = "${singularityDir}/ctat-splicing_${ctatSplicingVersion}.sif"
	ensemblVepImg      = "${singularityDir}/ensembl-vep_${ensemblVepVersion}.sif"
	fastpImg           = "${singularityDir}/fastp_${fastpVersion}.sif"
	fastqcImg          = "${singularityDir}/fastqc_${fastqcVersion}.sif"
	gatk4Img           = "${singularityDir}/gatk_${gatk4Version}.sif"
	gencoreImg         = "${singularityDir}/gencore_${gencoreVersion}.sif"
	mosdepthImg        = "${singularityDir}/mosdepth_${mosdepthVersion}.sif"
	msisensorproImg    = "${singularityDir}/msisensor-pro_${msisensorproVersion}.sif"
	multiqcImg         = "${singularityDir}/multiqc_${multiqcVersion}.sif"
